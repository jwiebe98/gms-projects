{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensions": {
    "serviceBus": {
      "transportType": "amqpWebSockets"
    },
    "queues": {
      "maxDequeueCount": 1
    }
  }
}